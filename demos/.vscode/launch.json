{
    "version": "0.2.0",
    "configurations": [
      {
        "preLaunchTask": "${defaultBuildTask}",
        "type": "probe-rs-debug",
        "request": "launch",
        "name": "probe_rs Debug",
        "cwd": "${workspaceFolder}",
        "runtimeExecutable": "probe-rs",
        "connectUnderReset": true,
        "chip": "STM32F103C8",
        "flashingConfig": {
          "flashingEnabled": true,
        },
        "coreConfigs": [
          {
            "programBinary": "target/thumbv7m-none-eabi/debug/rustngo-lib",
            "svdFile": ".vscode/stm32f103.svd",
            "rttEnabled": true,
          }
        ],
        "env": {
          //"RUST_LOG": "info"
        },
        "consoleLogLevel": "Console"
      }
    ]
}
